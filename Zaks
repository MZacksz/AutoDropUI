local function VNXVLOO_fake_script()
	local script = Instance.new('LocalScript', Main)

	_G.Drop = false
	_G.AutoDrops = false
	
	local Drop = script.Parent.Drop
	local AutoDrop = script.Parent.AutoDrop
	local Amount = script.Parent.Amount
	local title = script.Parent.Title
	local Warning = script.Parent.Warning
	
	Drop.MouseButton1Click:Connect(function()
		if _G.Drop == false then
			_G.Drop = true
			if Amount.Text == "" then
				Warning.Visible = true
				wait(1)
				Warning.Visible = false
			end
			local A_1 = "DropMoney"
			local A_2 = script.Parent.Amount.Text
			local Event = game:GetService("ReplicatedStorage").MainEvent
			Event:FireServer(A_1, A_2)
		else
			_G.Drop = false
		end
	end)
	
	script.Parent.AutoDrop.MouseButton1Click:Connect(function()
		if _G.AutoDrops == false and title.Text == "Auto Drop: OFF" then
			_G.AutoDrops = true
			title.Text = "Auto Drop: ON"
			if Amount.Text == "" then
				Warning.Visible = true
				wait(1)
				Warning.Visible = false
			end
			while true do
				local A_1 = "DropMoney"
				local A_2 = script.Parent.Amount.Text
				local Event = game:GetService("ReplicatedStorage").MainEvent
				Event:FireServer(A_1, A_2)
				wait(5)
				if _G.AutoDrops == false then
					break
				end
			end
		else
			_G.AutoDrops = false
			title.Text = "Auto Drop: OFF"
		end
	end)
end
coroutine.wrap(VNXVLOO_fake_script)()
